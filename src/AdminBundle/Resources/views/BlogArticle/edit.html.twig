<div class="box box-new-form">
    <div class="row">
        {{ form_start(form, {'attr': {'class': 'edit-form'}} ) }}
        {{ form_errors(form) }}

        {{ form_row(form.title) }}
        {{ form_row(form.handle) }}
        {{ form_row(form.dateToPublish, {'attr': {'class': 'dateSelect'}}) }}
        {{ form_row(form.content) }}

        {{ form_row(form.submit, {'attr': {'class': 'button edit'}} ) }}

        {{ form_end(form) }}
    </div>
</div>
